{
  "name": "Rsi-school",
  "productName": "Rsi-school",
  "version": "1.0.15",
  "description": "School management software",
  "author": "rsi-technologies",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lmrnba6/rsi-school-desktop.git"
  },
  "build": {
    "appId": "com.rsi-technologies.Rsi-school",
    "productName": "Rsi-school",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false
    },
    "files": [
      "!**/.vscode",
      "!**/build",
      "!**/config",
      "!**/assets/data",
      "!**/source"
    ],
    "extraResources": [
      {
        "from": "source/assets/data",
        "to": "dist/assets/data",
        "filter": "database*"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "test": "karma start ./config/karma.conf.js",
    "build:once": "webpack --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --win --publish never",
    "deploy": "electron-builder --win --publish always",
    "package": "electron-builder ."
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.1",
