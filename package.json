    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/pg": "^7.4.11",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.3",
    "core-js": "^2.5.7",
    "drivelist": "^6.4.3",
    "electron-updater": "^2.18.2",
